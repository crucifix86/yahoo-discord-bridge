[req]
default_bits = 2048
prompt = no
default_md = sha256
distinguished_name = dn
x509_extensions = v3_ext

[dn]
CN = login.yahoo.com

[v3_ext]
subjectAltName = @alt_names
basicConstraints = CA:TRUE

[alt_names]
DNS.1 = login.yahoo.com
DNS.2 = yahoo.com
DNS.3 = msg.yahoo.com
DNS.4 = vcs1.msg.yahoo.com
DNS.5 = vcs2.msg.yahoo.com
DNS.6 = scs.msg.yahoo.com
DNS.7 = scsa.msg.yahoo.com
DNS.8 = cs101.msg.sp1.yahoo.com
DNS.9 = insider.msg.yahoo.com
IP.1 = 192.168.1.121
